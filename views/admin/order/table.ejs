<!-- DataTales Example -->
<div class="card shadow mb-4">
  <div class="card-header py-3">
    <!-- <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6> -->
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table
        class="table table-bordered"
        id="dataTable"
        width="100%"
        cellspacing="0"
      >
        <thead>
          <tr>
            <th>No</th>
            <th>Invoice</th>
            <th>Customer</th>
            <th>Payment</th>
            <th>Payment Status</th>
            <th>Order Status</th>
            <th>Order Date</th>
            <th>Total</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% order.forEach(function(Order, index) { %>
          <tr>
            <td><%= index+1 %></td>
            <td><%= Order.invoice %></td>
            <td>
              <%= Order.memberId.firstName +" "+ Order.memberId.lastName %>
            </td>
            <td><%= Order.payments.bankFrom %></td>
            <td><%= Order.payments.status %></td>
            <td><%= Order.status %></td>

            <td><%= Order.orderDate.toLocaleString().substr(0, 9)%></td>
            <td><%= Order.total %></td>

            <td>
              <a
                href="/admin/order/<%=Order._id %>"
                class="btn btn-success btn-circle btn-sm"
                ><i class="fa fa-eye"></i>
              </a>
            </td>
          </tr>

          <% }); %>
        </tbody>
      </table>
    </div>
  </div>
</div>
